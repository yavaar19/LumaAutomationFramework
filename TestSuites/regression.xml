<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression">
    <listeners>
        <listener class-name="TestCase.Listeners"/>
    </listeners>
    <test verbose="2" parallel="classes" preserve-order="true"
          name="Login and Registration Page">
        <classes>
            <class name="TestCase.UnitTest.LogInPageTest"/>
            <class name="TestCase.UnitTest.RegistrationPageTest"/>
        </classes>
    </test>

    <test verbose="2" preserve-order="true"
          name="Account Page">
        <classes>
            <class name="TestCase.UnitTest.MyAccountPageTest"/>
        </classes>
    </test>
</suite>